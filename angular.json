{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "atividades-turisticas-portugal": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/atividades-turisticas-portugal",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": []
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "browserTarget": "atividades-turisticas-portugal:build",
          "options": {},
          "configurations": {
            "production": {
              "browserTarget": "atividades-turisticas-portugal:build:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "atividades-turisticas-portugal"
}
